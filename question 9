#include <stdio.h>
int main()
{
int a;
printf ("Input day number 1 to 7"); 
scanf("%d", &a);
if (a==1)
printf ("monday"); 
else if(a==2)
printf("Tuesday");
else if (a==3)
printf ("Wednesday"); 
else if (a==4)
printf("Thursday");
else if (a==5)
printf("Friday"); 
else if (a==6)
printf("saturday");
else
printf ("Sunday"); 
return 0;
}
